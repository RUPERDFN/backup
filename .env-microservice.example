# TheCookFlow Monetization Microservice Environment Variables
# Copy to .env and fill with your actual values

# Google Play Configuration
GOOGLE_PLAY_PACKAGE_NAME=com.cookflow.app
GOOGLE_PLAY_PRODUCT_ID=suscripcion
GOOGLE_PLAY_SERVICE_ACCOUNT_JSON_BASE64=<your-service-account-json-in-base64>

# CORS Configuration
WEB_ORIGIN=https://thecookflow.com

# Optional
SESSION_SECRET=<your-random-session-secret>
LOG_LEVEL=info
PORT=3000